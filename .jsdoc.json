{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["dist/js"],
    "exclude": ["docs", "hint-report", "node_modules"],
    "includePattern": ".+\\.js$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
